{
	"name": "forthehorde",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint . && prettier --check .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run",
		"format": "prettier --write .",
		"load:classes": "node --experimental-specifier-resolution=node --no-warnings --env-file=.env.local --loader ts-node/esm dev/load_classes.ts"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.3",
		"@eslint/js": "^9.17.0",
		"@sveltejs/adapter-vercel": "^5.5.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@types/node": "^22.12.0",
		"autoprefixer": "^10.4.20",
		"eslint": "^9.7.0",
		"eslint-config-prettier": "10.0.1",
		"eslint-plugin-svelte": "^2.36.0",
		"flowbite": "^3.0.0",
		"flowbite-svelte": "^0.47.4",
		"globals": "^15.0.0",
		"prettier": "^3.3.2",
		"prettier-plugin-svelte": "^3.2.6",
		"prettier-plugin-tailwindcss": "^0.6.10",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^3.4.17",
		"ts-node": "^10.9.2",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.0.0",
		"vite": "^5.4.11",
		"vitest": "2.1.9"
	},
	"dependencies": {
		"blizzard.js": "^4.6.0"
	}
}