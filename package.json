{
	"name": "forthehorde",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint . && prettier --check .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run",
		"format": "prettier --write .",
		"load:classes": "node --experimental-specifier-resolution=node --no-warnings --env-file=.env.local --loader ts-node/esm dev/load_classes.ts",
		"load:titles": "node --experimental-specifier-resolution=node --no-warnings --env-file=.env.local --loader ts-node/esm dev/titles.ts"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.6",
		"@eslint/js": "^9.20.0",
		"@sveltejs/adapter-vercel": "^5.6.2",
		"@sveltejs/kit": "^2.17.2",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@tailwindcss/vite": "^4.0.6",
		"@trigger.dev/sdk": "^3.3.16",
		"@types/node": "^22.13.4",
		"autoprefixer": "^10.4.20",
		"blizzard.js": "^4.6.0",
		"cookie": "1.0.2",
		"eslint": "^9.20.1",
		"eslint-config-prettier": "10.0.1",
		"eslint-plugin-svelte": "^2.46.1",
		"flowbite": "^3.1.2",
		"flowbite-svelte": "^0.47.4",
		"globals": "^15.15.0",
		"p-retry": "^6.2.1",
		"prettier": "^3.5.1",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"rate-limiter-flexible": "^5.0.5",
		"svelte": "^5.20.1",
		"svelte-check": "^4.1.4",
		"tailwindcss": "^4.0.6",
		"ts-node": "^10.9.2",
		"typescript": "^5.7.3",
		"typescript-eslint": "^8.24.1",
		"vite": "^6.1.0",
		"vite-plugin-triggerkit": "^1.2.0",
		"vitest": "3.0.5"
	}
}
